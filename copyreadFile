#include "monty.h"
char *readFile(char *filename)
{
	FILE *file;
	char *line;
	ssize_t lineLength;
	size_t lineSize = 0;

	file = fopen(filename, "r");

        if(file == NULL)
        {
                fprintf(stderr, "Error: Can't open file %s\n", filename);
                exit(EXIT_FAILURE);
        }
	
	/* Read and process each line of the file */

	while ((lineLength = getline(&line, &lineSize, file)) != -1)
        {
		printf("%s", line);
	}
	free(line);

	fclose(file);
	return(filename);
}
